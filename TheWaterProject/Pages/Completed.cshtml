@page
<div class="text-center">
    <h2>Thanks!</h2>
    <p>Thanks for placing order #@OrderId</p>
    <p>We'll send your donation to the projects you've chosen.</p>
    <a class="btn btn-primary" asp-controller="Home">Return to Projects</a>
</div>

@functions {

    [BindProperty(SupportsGet = true)]
    public string? OrderId { get; set; }
}
